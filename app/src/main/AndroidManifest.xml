<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.controlhouse.utopiasoft.controlhouse">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".menuPrincipal"
            android:label="@string/title_activity_menu_principal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Movimientos.activity_movimientos"
            android:label="@string/title_activity_movimientos"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustPan" />
        <activity
            android:name=".Cuentas.activity_cuentas"
            android:label="@string/title_activity_cuentas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Categorias.activity_categorias"
            android:label="@string/title_activity_categorias"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>